union Optional {
    class None {}
    class Some {
        var first : int32;
        var second : char;
    }
}

class Bag {
    var ival : int32;
    var fval : fp32;
}

function Bag.new(a : int32, b : fp32){
    this.ival = a;
    this.fval = b;
}

function Bag.add() {
    this.ival = this.ival + 1;
}

function main() {
    var bag : Bag = alloc(Bag(1, 2.0));
}

