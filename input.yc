function fact(a : int32) : int32 {
    if (a > 0) {
        return a * fact(a - 1);
    }
    return 1;
}

function main() {
    var i : int32 = 1+2*3;
    const j = "hello";
    var arr : int32[5];
    var k : int32 = fact(1);

    i = i + k;
    arr[2] = i;
    printf("%d %d\n", i, fact(i));
    i = 0;
    while (i < 12) {
        printf("%d %d\n", i, fact(i));
        i = i + 1;
    }
}
