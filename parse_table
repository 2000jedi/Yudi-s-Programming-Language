<STATEMENTS>
<PFUN> = <EPS>
<PFUN> = LPAR <PARS> RPAR
<ARITH> = STRING
<ARITH> = FLOAT
<ARITH> = INT
<ARITH> = NAME <PFUN>
<OPTIONAL_PARS> = <EPS>
<OPTIONAL_PARS> = COMMA <ARITH> <OPTIONAL_PARS>
<PARS> = <ARITH> <OPTIONAL_PARS>
<OPTIONAL_ASSIGN> = <EPS>
<OPTIONAL_ASSIGN> = EQU <ARITH>
<STATEMENT> = <ARITH> <OPTIONAL_ASSIGN> EOL
<STATEMENTS> = <EPS>
<STATEMENTS> = <STATEMENT> <STATEMENTS>
